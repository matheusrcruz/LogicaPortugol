algoritmo "Kreuza01"
var
anoAtual, anoNascimento, idade : inteiro


inicio
      escreva("Escreva em que ano nós estamos?:  ")
       leia(anoAtual)
      escreva("Em que ano eu nasci?: ")
       leia(anoNascimento)
       idade <- anoAtual-anoNascimento
                //escrevraaaau
       escreval("Minha idade será: ", idade)

fimalgoritmo