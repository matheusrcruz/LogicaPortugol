algoritmo "semnome"
var
imposto, gasto, total: real
inicio
      escreva("Qaunto eu comprei?: ")
      leia(gasto)
      imposto <- gasto*0.20
      total <- imposto + gasto
      escreva("Olha fora o que voce gasto R$",gasto," voce pagara R$", imposto,"Resultara em R$", total,"divida para o banco")

fimalgoritmo